(utt.save (utt.synth (Utterance Text "Author of the danger trail, Philip Steels, etc." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0001.utt")
(utt.save (utt.synth (Utterance Text "Not at this particular case, Tom, apologized Whittemore." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0002.utt")
(utt.save (utt.synth (Utterance Text "For the twentieth time that evening the two men shook hands." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0003.utt")
(utt.save (utt.synth (Utterance Text "Lord, but I'm glad to see you again, Phil." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0004.utt")
(utt.save (utt.synth (Utterance Text "Will we ever forget it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0005.utt")
(utt.save (utt.synth (Utterance Text "God bless 'em, I hope I'll go on seeing them forever." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0006.utt")
(utt.save (utt.synth (Utterance Text "And you always want to see it in the superlative degree." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0007.utt")
(utt.save (utt.synth (Utterance Text "Gad, your letter came just in time." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0008.utt")
(utt.save (utt.synth (Utterance Text "He turned sharply, and faced Gregson across the table." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0009.utt")
(utt.save (utt.synth (Utterance Text "I'm playing a single hand in what looks like a losing game." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0010.utt")
(utt.save (utt.synth (Utterance Text "If I ever needed a fighter in my life I need one now." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0011.utt")
(utt.save (utt.synth (Utterance Text "Gregson shoved back his chair and rose to his feet." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0012.utt")
(utt.save (utt.synth (Utterance Text "He was a head shorter than his companion, of almost delicate physique." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0013.utt")
(utt.save (utt.synth (Utterance Text "Now you're coming down to business, Phil, he exclaimed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0014.utt")
(utt.save (utt.synth (Utterance Text "It's the aurora borealis." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0015.utt")
(utt.save (utt.synth (Utterance Text "There's Fort Churchill, a rifle-shot beyond the ridge, asleep." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0016.utt")
(utt.save (utt.synth (Utterance Text "From that moment his friendship for Belize turns to hatred and jealousy." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0017.utt")
(utt.save (utt.synth (Utterance Text "There was a change now." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0018.utt")
(utt.save (utt.synth (Utterance Text "I followed the line of the proposed railroad, looking for chances." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0019.utt")
(utt.save (utt.synth (Utterance Text "Clubs and balls and cities grew to be only memories." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0020.utt")
(utt.save (utt.synth (Utterance Text "It fairly clubbed me into recognizing it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0021.utt")
(utt.save (utt.synth (Utterance Text "Hardly were our plans made public before we were met by powerful opposition." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0022.utt")
(utt.save (utt.synth (Utterance Text "A combination of Canadian capital quickly organized and petitioned for the same privileges." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0023.utt")
(utt.save (utt.synth (Utterance Text "It was my reports from the north which chiefly induced people to buy." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0024.utt")
(utt.save (utt.synth (Utterance Text "I was about to do this when cooler judgment prevailed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0025.utt")
(utt.save (utt.synth (Utterance Text "It occurred to me that there would have to be an accounting." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0026.utt")
(utt.save (utt.synth (Utterance Text "To my surprise he began to show actual enthusiasm in my favor." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0027.utt")
(utt.save (utt.synth (Utterance Text "Robbery, bribery, fraud, " )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0028.utt")
(utt.save (utt.synth (Utterance Text "Their forces were already moving into the north country." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0029.utt")
(utt.save (utt.synth (Utterance Text "I had faith in them." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0030.utt")
(utt.save (utt.synth (Utterance Text "They were three hundred yards apart." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0031.utt")
(utt.save (utt.synth (Utterance Text "Since then some mysterious force has been fighting us at every step." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0032.utt")
(utt.save (utt.synth (Utterance Text "He unfolded a long typewritten letter, and handed it to Gregson." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0033.utt")
(utt.save (utt.synth (Utterance Text "Men of Selden's stamp don't stop at women and children." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0034.utt")
(utt.save (utt.synth (Utterance Text "He stopped, and Philip nodded at the horrified question in his eyes." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0035.utt")
(utt.save (utt.synth (Utterance Text "She turned in at the hotel." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0036.utt")
(utt.save (utt.synth (Utterance Text "I was the only one who remained sitting." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0037.utt")
(utt.save (utt.synth (Utterance Text "We'll have to watch our chances." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0038.utt")
(utt.save (utt.synth (Utterance Text "The ship should be in within a week or ten days." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0039.utt")
(utt.save (utt.synth (Utterance Text "I suppose you wonder why she is coming up here." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0040.utt")
(utt.save (utt.synth (Utterance Text "Meanwhile I'll go out to breathe a spell." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0041.utt")
(utt.save (utt.synth (Utterance Text "How could he explain his possession of the sketch." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0042.utt")
(utt.save (utt.synth (Utterance Text "It seemed nearer to him since he had seen and talked with Gregson." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0043.utt")
(utt.save (utt.synth (Utterance Text "Her own betrayal of herself was like tonic to Philip." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0044.utt")
(utt.save (utt.synth (Utterance Text "He moved away as quietly as he had come." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0045.utt")
(utt.save (utt.synth (Utterance Text "The girl faced him, her eyes shining with sudden fear." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0046.utt")
(utt.save (utt.synth (Utterance Text "Close beside him gleamed the white fangs of the wolf-dog." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0047.utt")
(utt.save (utt.synth (Utterance Text "He looked at the handkerchief more, closely." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0048.utt")
(utt.save (utt.synth (Utterance Text "Gregson was asleep when he re-entered the cabin." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0049.utt")
(utt.save (utt.synth (Utterance Text "In spite of their absurdity the words affected Philip curiously." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0050.utt")
(utt.save (utt.synth (Utterance Text "The lace was of a delicate ivory color, faintly tinted with yellow." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0051.utt")
(utt.save (utt.synth (Utterance Text "It was a curious coincidence." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0052.utt")
(utt.save (utt.synth (Utterance Text "Suddenly his fingers closed tightly over the handkerchief." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0053.utt")
(utt.save (utt.synth (Utterance Text "There was nothing on the rock." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0054.utt")
(utt.save (utt.synth (Utterance Text "Philip stood undecided, his ears strained to catch the slightest sound." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0055.utt")
(utt.save (utt.synth (Utterance Text "Pearce's little eyes were fixed on him shrewdly." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0056.utt")
(utt.save (utt.synth (Utterance Text "I have no idea, replied Philip." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0057.utt")
(utt.save (utt.synth (Utterance Text "I came for information more out of curiosity than anything else." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0058.utt")
(utt.save (utt.synth (Utterance Text "His immaculate appearance was gone." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0059.utt")
(utt.save (utt.synth (Utterance Text "Anyway, no one saw her like that." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0060.utt")
(utt.save (utt.synth (Utterance Text "Philip snatched at the letter which Gregson held out to him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0061.utt")
(utt.save (utt.synth (Utterance Text "The men stared into each other's face." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0062.utt")
(utt.save (utt.synth (Utterance Text "Yes, it was a man who asked, a stranger." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0063.utt")
(utt.save (utt.synth (Utterance Text "The fourth and fifth days passed without any developments." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0064.utt")
(utt.save (utt.synth (Utterance Text "They closed now until his fingers were like cords of steel." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0065.utt")
(utt.save (utt.synth (Utterance Text "He saw Jeanne falter for a moment." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0066.utt")
(utt.save (utt.synth (Utterance Text "Surely I will excuse you, she cried." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0067.utt")
(utt.save (utt.synth (Utterance Text "In a flash Philip followed its direction." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0068.utt")
(utt.save (utt.synth (Utterance Text "It was his intention to return to Eileen and her father." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0069.utt")
(utt.save (utt.synth (Utterance Text "He would first hunt up Gregson and begin his work there." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0070.utt")
(utt.save (utt.synth (Utterance Text "What was the object of your little sensation." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0071.utt")
(utt.save (utt.synth (Utterance Text "But who was Eileen's double." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0072.utt")
(utt.save (utt.synth (Utterance Text "The promoter's eyes were heavy, with little puffy bags under them." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0073.utt")
(utt.save (utt.synth (Utterance Text "And now, down there, Eileen was waiting for him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0074.utt")
(utt.save (utt.synth (Utterance Text "There has been a change, she interrupted him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0075.utt")
(utt.save (utt.synth (Utterance Text "The gray eyes faltered; the flush deepened." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0076.utt")
(utt.save (utt.synth (Utterance Text "It is the fire, partly, she said." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0077.utt")
(utt.save (utt.synth (Utterance Text "Then, and at supper, he tried to fathom her." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0078.utt")
(utt.save (utt.synth (Utterance Text "It was a large canoe." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0079.utt")
(utt.save (utt.synth (Utterance Text "What if Jeanne failed him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0080.utt")
(utt.save (utt.synth (Utterance Text "What if she did not come to the rock." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0081.utt")
(utt.save (utt.synth (Utterance Text "His face was streaming with blood." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0082.utt")
(utt.save (utt.synth (Utterance Text "A shadow was creeping over Pierre's eyes." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0083.utt")
(utt.save (utt.synth (Utterance Text "Scarcely had he uttered the name when Pierre's closing eyes shot open." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0084.utt")
(utt.save (utt.synth (Utterance Text "A trickle of fresh blood ran over his face." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0085.utt")
(utt.save (utt.synth (Utterance Text "Death had come with terrible suddenness." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0086.utt")
(utt.save (utt.synth (Utterance Text "Philip bent lower, and stared into the face of the dead man." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0087.utt")
(utt.save (utt.synth (Utterance Text "He made sure that the magazine was loaded, and resumed his paddling." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0088.utt")
(utt.save (utt.synth (Utterance Text "The nightglow was treacherous to shoot by." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0089.utt")
(utt.save (utt.synth (Utterance Text "The singing voice approached rapidly." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0090.utt")
(utt.save (utt.synth (Utterance Text "His blood grew hot with rage at the thought." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0091.utt")
(utt.save (utt.synth (Utterance Text "He went down in midstream, searching the shadows of both shores." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0092.utt")
(utt.save (utt.synth (Utterance Text "For a full minute he crouched and listened." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0093.utt")
(utt.save (utt.synth (Utterance Text "He had barely entered this when he saw the glow of a fire." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0094.utt")
(utt.save (utt.synth (Utterance Text "A big canvas tent was the first thing to come within his vision." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0095.utt")
(utt.save (utt.synth (Utterance Text "Perhaps she had already met her fate a little deeper in the forest." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0096.utt")
(utt.save (utt.synth (Utterance Text "Then you can arrange yourself comfortably among these robes in the bow." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0097.utt")
(utt.save (utt.synth (Utterance Text "Shall I carry you." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0098.utt")
(utt.save (utt.synth (Utterance Text "A maddening joy pounded in his brain." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0099.utt")
(utt.save (utt.synth (Utterance Text "You must sleep, he urged." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0100.utt")
(utt.save (utt.synth (Utterance Text "You, you would not keep the truth from me." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0101.utt")
(utt.save (utt.synth (Utterance Text "He will follow us soon." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0102.utt")
(utt.save (utt.synth (Utterance Text "But there came no promise from the bow of the canoe." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0103.utt")
(utt.save (utt.synth (Utterance Text "She was sleeping under his protection as sweetly as a child." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0104.utt")
(utt.save (utt.synth (Utterance Text "Only, it is so wonderful, so almost impossible to believe." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0105.utt")
(utt.save (utt.synth (Utterance Text "The emotion which she had suppressed burst forth now in a choking sob." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0106.utt")
(utt.save (utt.synth (Utterance Text "If you only could know how I thank you." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0107.utt")
(utt.save (utt.synth (Utterance Text "He waded into the edge of the water and began scrubbing himself." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0108.utt")
(utt.save (utt.synth (Utterance Text "Do you know that you are shaking my confidence in you." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0109.utt")
(utt.save (utt.synth (Utterance Text "Much, replied Jeanne, as tersely." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0110.utt")
(utt.save (utt.synth (Utterance Text "Instead, he joined her; and they ate like two hungry children." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0111.utt")
(utt.save (utt.synth (Utterance Text "He was wounded in the arm." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0112.utt")
(utt.save (utt.synth (Utterance Text "I suppose you picked that lingo up among the Indians." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0113.utt")
(utt.save (utt.synth (Utterance Text "Her words sent a strange chill through Philip." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0114.utt")
(utt.save (utt.synth (Utterance Text "He had no excuse for the feelings which were aroused in him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0115.utt")
(utt.save (utt.synth (Utterance Text "Was it the rendezvous of those who were striving to work his ruin." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0116.utt")
(utt.save (utt.synth (Utterance Text "She added, with genuine sympathy in her face and voice." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0117.utt")
(utt.save (utt.synth (Utterance Text "Pierre obeys me when we are together." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0118.utt")
(utt.save (utt.synth (Utterance Text "Jeanne was turning the bow shoreward." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0119.utt")
(utt.save (utt.synth (Utterance Text "My right foot feels like that of a Chinese debutante." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0120.utt")
(utt.save (utt.synth (Utterance Text "They ate dinner at the fifth, and rested for two hours." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0121.utt")
(utt.save (utt.synth (Utterance Text "Two years ago I gave up civilization for this." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0122.utt")
(utt.save (utt.synth (Utterance Text "She had died from cold and starvation." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0123.utt")
(utt.save (utt.synth (Utterance Text "It was Jeanne singing softly over beyond the rocks." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0124.utt")
(utt.save (utt.synth (Utterance Text "He was determined now to maintain a more certain hold upon himself." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0125.utt")
(utt.save (utt.synth (Utterance Text "Each day she became a more vital part of him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0126.utt")
(utt.save (utt.synth (Utterance Text "It was a temptation, but he resisted it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0127.utt")
(utt.save (utt.synth (Utterance Text "This one hope was destroyed as quickly as it was born." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0128.utt")
(utt.save (utt.synth (Utterance Text "Her face was against his breast." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0129.utt")
(utt.save (utt.synth (Utterance Text "She was his now, forever." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0130.utt")
(utt.save (utt.synth (Utterance Text "Providence had delivered him through the maelstrom." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0131.utt")
(utt.save (utt.synth (Utterance Text "A cry of joy burst from Philip's lips." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0132.utt")
(utt.save (utt.synth (Utterance Text "Philip began to feel that he had foolishly overestimated his strength." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0133.utt")
(utt.save (utt.synth (Utterance Text "He obeyed the pressure of her hand." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0134.utt")
(utt.save (utt.synth (Utterance Text "I am going to surprise father, and you will go with Pierre." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0135.utt")
(utt.save (utt.synth (Utterance Text "About him, everywhere, were the evidences of luxury and of age." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0136.utt")
(utt.save (utt.synth (Utterance Text "Then he stepped back with a low cry of pleasure." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0137.utt")
(utt.save (utt.synth (Utterance Text "In the picture he saw each moment a greater resemblance to Jeanne." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0138.utt")
(utt.save (utt.synth (Utterance Text "He told himself that as he washed himself and groomed his disheveled clothes." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0139.utt")
(utt.save (utt.synth (Utterance Text "Accept a father's blessing, and with it, this." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0140.utt")
(utt.save (utt.synth (Utterance Text "It seems like a strange pointing of the hand of God." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0141.utt")
(utt.save (utt.synth (Utterance Text "Such things had occurred before, he told Philip." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0142.utt")
(utt.save (utt.synth (Utterance Text "Ah, I had forgotten, he exclaimed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0143.utt")
(utt.save (utt.synth (Utterance Text "But there was something even more startling than this resemblance." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0144.utt")
(utt.save (utt.synth (Utterance Text "I have to be careful of them, as they tear very easily." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0145.utt")
(utt.save (utt.synth (Utterance Text "Of course, that is uninteresting, she continued." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0146.utt")
(utt.save (utt.synth (Utterance Text "A moment before he was intoxicated by a joy that was almost madness." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0147.utt")
(utt.save (utt.synth (Utterance Text "Now these things had been struck dead within him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0148.utt")
(utt.save (utt.synth (Utterance Text "For an instant he saw Pierre drawn like a silhouette against the sky." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0149.utt")
(utt.save (utt.synth (Utterance Text "Goodbye, Pierre, he shouted." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0150.utt")
(utt.save (utt.synth (Utterance Text "And MacDougall was beyond the trail, with three weeks to spare." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0151.utt")
(utt.save (utt.synth (Utterance Text "Philip thrust himself against it and entered." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0152.utt")
(utt.save (utt.synth (Utterance Text "MacDougall tapped his forehead suspiciously with a stubby forefinger." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0153.utt")
(utt.save (utt.synth (Utterance Text "He was smooth-shaven, and his hair and eyes were black." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0154.utt")
(utt.save (utt.synth (Utterance Text "Won't you draw up, gentlemen." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0155.utt")
(utt.save (utt.synth (Utterance Text "A strange fire burned in his eyes when Thorpe turned." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0156.utt")
(utt.save (utt.synth (Utterance Text "He had worshiped her, as Dante might have worshiped Beatrice." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0157.utt")
(utt.save (utt.synth (Utterance Text "Does that look good." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0158.utt")
(utt.save (utt.synth (Utterance Text "They look as though he had been drumming a piano all his life." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0159.utt")
(utt.save (utt.synth (Utterance Text "You want to go over and see his gang throw dirt." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0160.utt")
(utt.save (utt.synth (Utterance Text "Take away their foreman and they wouldn't be worth their grub." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0161.utt")
(utt.save (utt.synth (Utterance Text "That's the sub-foreman, explained Thorpe." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0162.utt")
(utt.save (utt.synth (Utterance Text "Philip made no effort to follow." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0163.utt")
(utt.save (utt.synth (Utterance Text "He came first a year ago, and revealed himself to Jeanne." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0164.utt")
(utt.save (utt.synth (Utterance Text "They are to attack your camp tomorrow night." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0165.utt")
(utt.save (utt.synth (Utterance Text "Two days ago Jeanne learned where her father's men were hiding." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0166.utt")
(utt.save (utt.synth (Utterance Text "I was near the cabin, and saw you." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0167.utt")
(utt.save (utt.synth (Utterance Text "Low bush whipped him in the face and left no sting." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0168.utt")
(utt.save (utt.synth (Utterance Text "Suddenly Jeanne stopped for an instant." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0169.utt")
(utt.save (utt.synth (Utterance Text "There was none of the joy of meeting in his face." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0170.utt")
(utt.save (utt.synth (Utterance Text "And when you come back in a few days, bring Eileen." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0171.utt")
(utt.save (utt.synth (Utterance Text "Gregson had left the outer door slightly ajar." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0172.utt")
(utt.save (utt.synth (Utterance Text "The date was nearly eighteen years old." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0173.utt")
(utt.save (utt.synth (Utterance Text "They were the presage of storm." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0174.utt")
(utt.save (utt.synth (Utterance Text "Down there the earth was already swelling with life." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0175.utt")
(utt.save (utt.synth (Utterance Text "For the first time in his life he was yearning for a scrap." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0176.utt")
(utt.save (utt.synth (Utterance Text "She had been thoroughly and efficiently mauled." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0177.utt")
(utt.save (utt.synth (Utterance Text "Every bone in her aged body seemed broken or dislocated." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0178.utt")
(utt.save (utt.synth (Utterance Text "Tomorrow I'm going after that bear, he said." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0179.utt")
(utt.save (utt.synth (Utterance Text "If not, let's say our prayers and go to bed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0180.utt")
(utt.save (utt.synth (Utterance Text "So cheer up, and give us your paw." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0181.utt")
(utt.save (utt.synth (Utterance Text "This time he did not yap for mercy." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0182.utt")
(utt.save (utt.synth (Utterance Text "And the air was growing chilly." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0183.utt")
(utt.save (utt.synth (Utterance Text "Don't you see, I'm chewing this thing in two." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0184.utt")
(utt.save (utt.synth (Utterance Text "The questions may have come vaguely in his mind." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0185.utt")
(utt.save (utt.synth (Utterance Text "Like a flash he launched himself into the feathered mass of the owl." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0186.utt")
(utt.save (utt.synth (Utterance Text "Ahead of them they saw a glimmer of sunshine." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0187.utt")
(utt.save (utt.synth (Utterance Text "Two gigantic owls were tearing at the carcass." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0188.utt")
(utt.save (utt.synth (Utterance Text "The big-eyed, clucking moose-birds were most annoying." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0189.utt")
(utt.save (utt.synth (Utterance Text "Next to them the Canada jays were most persistent." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0190.utt")
(utt.save (utt.synth (Utterance Text "For a time the exciting thrill of his adventure was gone." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0191.utt")
(utt.save (utt.synth (Utterance Text "He did not rush in." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0192.utt")
(utt.save (utt.synth (Utterance Text "It was edged with ice." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0193.utt")
(utt.save (utt.synth (Utterance Text "He drank of the water cautiously." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0194.utt")
(utt.save (utt.synth (Utterance Text "But a strange thing happened." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0195.utt")
(utt.save (utt.synth (Utterance Text "He began to follow the footprints of the dog." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0196.utt")
(utt.save (utt.synth (Utterance Text "Such a dog the wise driver kills, or turns loose." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0197.utt")
(utt.save (utt.synth (Utterance Text "Sometimes her dreams were filled with visions." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0198.utt")
(utt.save (utt.synth (Utterance Text "Thus had the raw wilderness prepared him for this day." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0199.utt")
(utt.save (utt.synth (Utterance Text "He leapt again, and the club caught him once more." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0200.utt")
(utt.save (utt.synth (Utterance Text "He cried, and swung the club wildly." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0201.utt")
(utt.save (utt.synth (Utterance Text "She turned, fearing that Jacques might see what was in her face." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0202.utt")
(utt.save (utt.synth (Utterance Text "They were following the shore of a lake." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0203.utt")
(utt.save (utt.synth (Utterance Text "The wolf-dog thrust his gaunt muzzle toward him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0204.utt")
(utt.save (utt.synth (Utterance Text "From now on we're pals." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0205.utt")
(utt.save (utt.synth (Utterance Text "He says he bought him of Jacques Le Beau." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0206.utt")
(utt.save (utt.synth (Utterance Text "How much was it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0207.utt")
(utt.save (utt.synth (Utterance Text "Youth had come back to her, freed from the yoke of oppression." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0208.utt")
(utt.save (utt.synth (Utterance Text "It was not a large lake, and almost round." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0209.utt")
(utt.save (utt.synth (Utterance Text "Its diameter was not more than two hundred yards." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0210.utt")
(utt.save (utt.synth (Utterance Text "It drowned all sound that brute agony and death may have made." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0211.utt")
(utt.save (utt.synth (Utterance Text "Fresh cases, still able to walk, they clustered about the spokesman." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0212.utt")
(utt.save (utt.synth (Utterance Text "Between him and the beach was the cane-grass fence of the compound." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0213.utt")
(utt.save (utt.synth (Utterance Text "Besides, he was paid one case of tobacco per head." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0214.utt")
(utt.save (utt.synth (Utterance Text "They die out of spite." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0215.utt")
(utt.save (utt.synth (Utterance Text "The other felt a sudden wave of irritation rush through him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0216.utt")
(utt.save (utt.synth (Utterance Text "Oppressive as the heat had been, it was now even more oppressive." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0217.utt")
(utt.save (utt.synth (Utterance Text "The ringing of the big bell aroused him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0218.utt")
(utt.save (utt.synth (Utterance Text "At first he puzzled over something untoward he was sure had happened." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0219.utt")
(utt.save (utt.synth (Utterance Text "A dead man is of no use on a plantation." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0220.utt")
(utt.save (utt.synth (Utterance Text "I don't know why you're here at all." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0221.utt")
(utt.save (utt.synth (Utterance Text "What part of the United States is your home." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0222.utt")
(utt.save (utt.synth (Utterance Text "My, I'm almost homesick for it already." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0223.utt")
(utt.save (utt.synth (Utterance Text "She nodded, and her eyes grew soft and moist." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0224.utt")
(utt.save (utt.synth (Utterance Text "I was brought up the way most girls in Hawaii are brought up." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0225.utt")
(utt.save (utt.synth (Utterance Text "That came before my A B C's." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0226.utt")
(utt.save (utt.synth (Utterance Text "It was the same way with our revolvers and rifles." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0227.utt")
(utt.save (utt.synth (Utterance Text "But it contributed to the smash." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0228.utt")
(utt.save (utt.synth (Utterance Text "The last one I knew was an overseer." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0229.utt")
(utt.save (utt.synth (Utterance Text "Do you know any good land around here." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0230.utt")
(utt.save (utt.synth (Utterance Text "The Resident Commissioner is away in Australia." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0231.utt")
(utt.save (utt.synth (Utterance Text "I cannot follow you, she said." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0232.utt")
(utt.save (utt.synth (Utterance Text "I never allow what can't be changed to annoy me." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0233.utt")
(utt.save (utt.synth (Utterance Text "Why, the average review is more nauseating than cod liver oil." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0234.utt")
(utt.save (utt.synth (Utterance Text "His voice was passionately rebellious." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0235.utt")
(utt.save (utt.synth (Utterance Text "Don't you see I hate you." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0236.utt")
(utt.save (utt.synth (Utterance Text "So Hughie and I did the managing ourselves." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0237.utt")
(utt.save (utt.synth (Utterance Text "It happened to him at the Gallina Society in Oakland one afternoon." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0238.utt")
(utt.save (utt.synth (Utterance Text "He cried in such genuine dismay that she broke into hearty laughter." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0239.utt")
(utt.save (utt.synth (Utterance Text "Wash your hands of me." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0240.utt")
(utt.save (utt.synth (Utterance Text "I think it's much nicer to quarrel." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0241.utt")
(utt.save (utt.synth (Utterance Text "I saw it when she rolled." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0242.utt")
(utt.save (utt.synth (Utterance Text "I only read the quotations." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0243.utt")
(utt.save (utt.synth (Utterance Text "He was the soul of devotion to his employers." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0244.utt")
(utt.save (utt.synth (Utterance Text "Out of his eighteen hundred, he laid aside sixteen hundred each year." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0245.utt")
(utt.save (utt.synth (Utterance Text "You have heard always how he was the lover of the Princess Naomi." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0246.utt")
(utt.save (utt.synth (Utterance Text "They ought to pass here some time today." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0247.utt")
(utt.save (utt.synth (Utterance Text "I had been sad too long already." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0248.utt")
(utt.save (utt.synth (Utterance Text "All eyes, however, were staring at him in certitude of expectancy." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0249.utt")
(utt.save (utt.synth (Utterance Text "He had observed the business life of Hawaii and developed a vaulting ambition." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0250.utt")
(utt.save (utt.synth (Utterance Text "I may manage to freight a cargo back as well." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0251.utt")
(utt.save (utt.synth (Utterance Text "O'Brien had been a clean living young man with ideals." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0252.utt")
(utt.save (utt.synth (Utterance Text "He it was that lived to found the family of the Patino." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0253.utt")
(utt.save (utt.synth (Utterance Text "Straight out they swam, their heads growing smaller and smaller." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0254.utt")
(utt.save (utt.synth (Utterance Text "You won't die of malnutrition, be sure of that." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0255.utt")
(utt.save (utt.synth (Utterance Text "See the length of the body and that elongated neck." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0256.utt")
(utt.save (utt.synth (Utterance Text "They are coming ashore, whoever they are." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0257.utt")
(utt.save (utt.synth (Utterance Text "Soaked in seawater they offset the heat rays." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0258.utt")
(utt.save (utt.synth (Utterance Text "Think of investing in such an adventure." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0259.utt")
(utt.save (utt.synth (Utterance Text "Nobody knew his history, they of the Junta least of all." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0260.utt")
(utt.save (utt.synth (Utterance Text "I have been doubly baptized." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0261.utt")
(utt.save (utt.synth (Utterance Text "They wouldn't be sweeping a big vessel like the Martha." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0262.utt")
(utt.save (utt.synth (Utterance Text "Joan looked triumphantly at Sheldon, who bowed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0263.utt")
(utt.save (utt.synth (Utterance Text "And I hope you've got plenty of chain out, Captain Young." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0264.utt")
(utt.save (utt.synth (Utterance Text "The discovery seemed to have been made on the spur of the moment." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0265.utt")
(utt.save (utt.synth (Utterance Text "They handled two men already, both grub-thieves." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0266.utt")
(utt.save (utt.synth (Utterance Text "Eli Harding asked, as Shunk started to follow." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0267.utt")
(utt.save (utt.synth (Utterance Text "Now go ahead and tell me in a straightforward way what has happened." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0268.utt")
(utt.save (utt.synth (Utterance Text "That's where they cut off the Scottish Chiefs and killed all hands." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0269.utt")
(utt.save (utt.synth (Utterance Text "And after the bath a shave would not be bad." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0270.utt")
(utt.save (utt.synth (Utterance Text "Now please give a plain statement of what occurred." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0271.utt")
(utt.save (utt.synth (Utterance Text "You can take a vacation on pay." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0272.utt")
(utt.save (utt.synth (Utterance Text "They are big trees and require plenty of room." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0273.utt")
(utt.save (utt.synth (Utterance Text "And Raoul listened again to the tale of the house." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0274.utt")
(utt.save (utt.synth (Utterance Text "There are no kiddies and half grown youths among them." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0275.utt")
(utt.save (utt.synth (Utterance Text "Oolong Atoll was one hundred and forty miles in circumference." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0276.utt")
(utt.save (utt.synth (Utterance Text "McCoy found a stifling, poisonous atmosphere in the pent cabin." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0277.utt")
(utt.save (utt.synth (Utterance Text "It would give me nervous prostration." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0278.utt")
(utt.save (utt.synth (Utterance Text "She said with chattering teeth." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0279.utt")
(utt.save (utt.synth (Utterance Text "I'll be out of my head in fifteen minutes." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0280.utt")
(utt.save (utt.synth (Utterance Text "I do not blame you for anything; remember that." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0281.utt")
(utt.save (utt.synth (Utterance Text "If you mean to insinuate -- Brentwood began hotly." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0282.utt")
(utt.save (utt.synth (Utterance Text "The woman in you is only incidental, accidental, and irrelevant." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0283.utt")
(utt.save (utt.synth (Utterance Text "There was no forecasting this strange girl's processes." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0284.utt")
(utt.save (utt.synth (Utterance Text "But what they want with your toothbrush is more than I can imagine." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0285.utt")
(utt.save (utt.synth (Utterance Text "Give them their choice between a fine or an official whipping." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0286.utt")
(utt.save (utt.synth (Utterance Text "Keep an eye on him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0287.utt")
(utt.save (utt.synth (Utterance Text "Those are my oysters, he said at last." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0288.utt")
(utt.save (utt.synth (Utterance Text "They are not regular oyster pirates, Nicholas continued." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0289.utt")
(utt.save (utt.synth (Utterance Text "One by one the boys were captured." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0290.utt")
(utt.save (utt.synth (Utterance Text "The weeks had gone by, and no overt acts had been attempted." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0291.utt")
(utt.save (utt.synth (Utterance Text "Here, in the midmorning, the first casualty occurred." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0292.utt")
(utt.save (utt.synth (Utterance Text "They were deep in the primeval forest." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0293.utt")
(utt.save (utt.synth (Utterance Text "He had been foiled in his attempt to escape." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0294.utt")
(utt.save (utt.synth (Utterance Text "And twenty men could hold it with spears and arrows." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0295.utt")
(utt.save (utt.synth (Utterance Text "Bassett was a fastidious man." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0296.utt")
(utt.save (utt.synth (Utterance Text "There's a big English general right now whose name is Roberts." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0297.utt")
(utt.save (utt.synth (Utterance Text "This tacit promise of continued acquaintance gave Saxon a little joy-thrill." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0298.utt")
(utt.save (utt.synth (Utterance Text "I tell you I am disgusted with this adventure tomfoolery and rot." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0299.utt")
(utt.save (utt.synth (Utterance Text "From my earliest recollection my sleep was a period of terror." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0300.utt")
(utt.save (utt.synth (Utterance Text "But all my dreams violated this law." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0301.utt")
(utt.save (utt.synth (Utterance Text "It is very plausible to such people, a most convincing hypothesis." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0302.utt")
(utt.save (utt.synth (Utterance Text "But they make the mistake of ignoring their own duality." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0303.utt")
(utt.save (utt.synth (Utterance Text "I graduated last of my class." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0304.utt")
(utt.save (utt.synth (Utterance Text "They had no fixed values, to be altered by adjectives and adverbs." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0305.utt")
(utt.save (utt.synth (Utterance Text "He was pressing beyond the limits of his vocabulary." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0306.utt")
(utt.save (utt.synth (Utterance Text "Very early in my life, I separated from my mother." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0307.utt")
(utt.save (utt.synth (Utterance Text "His infernal chattering worries me even now as I think of it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0308.utt")
(utt.save (utt.synth (Utterance Text "White Leghorns, said Mrs Mortimer." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0309.utt")
(utt.save (utt.synth (Utterance Text "Massage under tension, was the cryptic reply." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0310.utt")
(utt.save (utt.synth (Utterance Text "Therefore, hurrah for the game." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0311.utt")
(utt.save (utt.synth (Utterance Text "It lived in perpetual apprehension of that quarter of the compass." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0312.utt")
(utt.save (utt.synth (Utterance Text "Broken-Tooth yelled with fright and pain." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0313.utt")
(utt.save (utt.synth (Utterance Text "Thus was momentum gained in the Younger World." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0314.utt")
(utt.save (utt.synth (Utterance Text "Saxon waited, for she knew a fresh idea had struck Billy." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0315.utt")
(utt.save (utt.synth (Utterance Text "We had been chased by them ourselves, more than once." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0316.utt")
(utt.save (utt.synth (Utterance Text "He was a wise hyena." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0317.utt")
(utt.save (utt.synth (Utterance Text "Production is doubling and quadrupling upon itself." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0318.utt")
(utt.save (utt.synth (Utterance Text "And the Edinburgh Evening News says, with editorial gloom." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0319.utt")
(utt.save (utt.synth (Utterance Text "With my strength I slammed it full into Red-Eye's face." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0320.utt")
(utt.save (utt.synth (Utterance Text "The log on which Lop-Ear was lying got adrift." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0321.utt")
(utt.save (utt.synth (Utterance Text "This is a common experience with all of us." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0322.utt")
(utt.save (utt.synth (Utterance Text "He considered the victory already his and stepped forward to the meat." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0323.utt")
(utt.save (utt.synth (Utterance Text "It was not Red-Eye's way to forego revenge so easily." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0324.utt")
(utt.save (utt.synth (Utterance Text "Whiz-zip-bang. Lop-Ear screamed with sudden anguish." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0325.utt")
(utt.save (utt.synth (Utterance Text "Cherokee identified himself with his instinct." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0326.utt")
(utt.save (utt.synth (Utterance Text "They were less stooped than we, less springy in their movements." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0327.utt")
(utt.save (utt.synth (Utterance Text "The Fire People, like ourselves, lived in caves." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0328.utt")
(utt.save (utt.synth (Utterance Text "Ah, indeed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0329.utt")
(utt.save (utt.synth (Utterance Text "Red-Eye never committed a more outrageous deed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0330.utt")
(utt.save (utt.synth (Utterance Text "Poor little Crooked-Leg was terribly scared." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0331.utt")
(utt.save (utt.synth (Utterance Text "Unconsciously, our yells and exclamations yielded to this rhythm." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0332.utt")
(utt.save (utt.synth (Utterance Text "This is no place for you." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0333.utt")
(utt.save (utt.synth (Utterance Text "He'll knock you off a few sticks in no time." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0334.utt")
(utt.save (utt.synth (Utterance Text "Red-Eye swung back and forth on the branch farther down." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0335.utt")
(utt.save (utt.synth (Utterance Text "So unexpected was my charge that I knocked him off his feet." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0336.utt")
(utt.save (utt.synth (Utterance Text "Encouraged by my conduct, Big-Face became a sudden ally." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0337.utt")
(utt.save (utt.synth (Utterance Text "The fighting had now become intermittent." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0338.utt")
(utt.save (utt.synth (Utterance Text "They obeyed him, and went here and there at his commands." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0339.utt")
(utt.save (utt.synth (Utterance Text "It was like the beating of hoofs." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0340.utt")
(utt.save (utt.synth (Utterance Text "Why, doggone you all, shake again." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0341.utt")
(utt.save (utt.synth (Utterance Text "Seventeen, no, eighteen days ago." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0342.utt")
(utt.save (utt.synth (Utterance Text "You mean for this State, General, Alberta." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0343.utt")
(utt.save (utt.synth (Utterance Text "He seemed to fill it with his tremendous vitality." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0344.utt")
(utt.save (utt.synth (Utterance Text "She was trying to pass the apron string around him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0345.utt")
(utt.save (utt.synth (Utterance Text "Get down and dig in." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0346.utt")
(utt.save (utt.synth (Utterance Text "They are greatly delighted with anything that is bright or giveth a sound." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0347.utt")
(utt.save (utt.synth (Utterance Text "They only lifted seven hundred and fifty." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0348.utt")
(utt.save (utt.synth (Utterance Text "It was simple, in its way, and no virtue of his." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0349.utt")
