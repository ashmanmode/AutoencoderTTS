(utt.save (utt.synth (Utterance Text "I just do appreciate it without being able to express my feelings." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0368.utt")
(utt.save (utt.synth (Utterance Text "In partnership with Daylight, the pair raided the San Jose Interurban." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0369.utt")
(utt.save (utt.synth (Utterance Text "He saw all men in the business game doing this." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0370.utt")
