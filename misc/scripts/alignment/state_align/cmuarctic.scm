(utt.save (utt.synth (Utterance Text "I sailed third mate in the little Vampire before you were born." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0474.utt")
(utt.save (utt.synth (Utterance Text "His outstretched arm dropped to his side, and he paused." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0475.utt")
(utt.save (utt.synth (Utterance Text "At this moment I felt a stir at my shoulder." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0476.utt")
(utt.save (utt.synth (Utterance Text "Wada, Louis, and the steward are servants of Asiatic breed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0477.utt")
(utt.save (utt.synth (Utterance Text "Also, she has forbidden them smoking their pipes in the after-room." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0478.utt")
(utt.save (utt.synth (Utterance Text "I tried to read George Moore last night, and was dreadfully bored." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0479.utt")
(utt.save (utt.synth (Utterance Text "Tom Spink has a harpoon." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0480.utt")
(utt.save (utt.synth (Utterance Text "Nimrod replied, with a slight manifestation of sensitiveness." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0481.utt")
(utt.save (utt.synth (Utterance Text "And their chief virtue lies in that they will never wear out." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0482.utt")
(utt.save (utt.synth (Utterance Text "Beyond dispute, Corry Hutchinson had married Mabel Holmes." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0483.utt")
(utt.save (utt.synth (Utterance Text "No-sir-ee." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0484.utt")
(utt.save (utt.synth (Utterance Text "Each insult added to the value of the claim." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0485.utt")
(utt.save (utt.synth (Utterance Text "For the rest, he was a mere automaton." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0486.utt")
(utt.save (utt.synth (Utterance Text "The river bared its bosom, and snorting steamboats challenged the wilderness." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0487.utt")
(utt.save (utt.synth (Utterance Text "Their love burned with increasing brightness." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0488.utt")
(utt.save (utt.synth (Utterance Text "They were artists, not biologists." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0489.utt")
(utt.save (utt.synth (Utterance Text "Both Johnny and his mother shuffled their feet as they walked." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0490.utt")
(utt.save (utt.synth (Utterance Text "And as in denial of guilt, the one-legged boy replied." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0491.utt")
(utt.save (utt.synth (Utterance Text "Burnt out like the crater of a volcano." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0492.utt")
(utt.save (utt.synth (Utterance Text "The boy, O'Brien, was specially maltreated." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0493.utt")
(utt.save (utt.synth (Utterance Text "O'Brien took off his coat and bared his right arm." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0494.utt")
(utt.save (utt.synth (Utterance Text "He bore no grudges and had few enemies." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0495.utt")
(utt.save (utt.synth (Utterance Text "And Tom King patiently endured." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0496.utt")
(utt.save (utt.synth (Utterance Text "King took every advantage he knew." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0497.utt")
(utt.save (utt.synth (Utterance Text "The lines were now very taut." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0498.utt")
(utt.save (utt.synth (Utterance Text "And right there I saw and knew it all." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0499.utt")
(utt.save (utt.synth (Utterance Text "Who the devil gave it to you to be judge and jury." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0500.utt")
(utt.save (utt.synth (Utterance Text "You're joking me, sir, the other managed to articulate." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0501.utt")
(utt.save (utt.synth (Utterance Text "Anything unusual or abnormal was sufficient to send a fellow to Molokai." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0502.utt")
(utt.save (utt.synth (Utterance Text "His beady black eyes saw bargains where other men saw bankruptcy." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0503.utt")
(utt.save (utt.synth (Utterance Text "He was an athlete and a giant." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0504.utt")
(utt.save (utt.synth (Utterance Text "We fished sharks on Niihau together." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0505.utt")
(utt.save (utt.synth (Utterance Text "The Claudine was leaving next morning for Honolulu." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0506.utt")
(utt.save (utt.synth (Utterance Text "In short, my joyous individualism was dominated by the orthodox bourgeois ethics." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0507.utt")
(utt.save (utt.synth (Utterance Text "Soon shall it be thrust back from off prostrate humanity." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0508.utt")
(utt.save (utt.synth (Utterance Text "Yet, in accordance with Ernest's test of truth, it worked." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0509.utt")
(utt.save (utt.synth (Utterance Text "Much more Ernest told them of themselves and of his disillusionment." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0510.utt")
(utt.save (utt.synth (Utterance Text "There is more behind this than a mere university ideal." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0511.utt")
(utt.save (utt.synth (Utterance Text "No, it is a palace, wherein there are many servants." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0512.utt")
(utt.save (utt.synth (Utterance Text "We must give ourselves and not our money alone." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0513.utt")
(utt.save (utt.synth (Utterance Text "We are consumed in our own flesh-pots." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0514.utt")
(utt.save (utt.synth (Utterance Text "But here amongst ourselves let us speak out." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0515.utt")
(utt.save (utt.synth (Utterance Text "Also, there was awe in their faces." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0516.utt")
(utt.save (utt.synth (Utterance Text "Out of abstractions Ernest had conjured a vision and made them see it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0517.utt")
(utt.save (utt.synth (Utterance Text "Illuminating oil was becoming all profit." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0518.utt")
(utt.save (utt.synth (Utterance Text "Such an act was in direct violation of the laws of the land." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0519.utt")
(utt.save (utt.synth (Utterance Text "He was fond of quoting a fragment from a certain poem." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0520.utt")
(utt.save (utt.synth (Utterance Text "Without them he could not run his empire." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0521.utt")
(utt.save (utt.synth (Utterance Text "For such countries nothing remained but reorganization." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0522.utt")
(utt.save (utt.synth (Utterance Text "They could not continue their method of producing surpluses." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0523.utt")
(utt.save (utt.synth (Utterance Text "At once would be instituted a dozen cooperative commonwealth states." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0524.utt")
(utt.save (utt.synth (Utterance Text "The Oligarchy wanted violence, and it set its agents provocateurs to work." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0525.utt")
(utt.save (utt.synth (Utterance Text "Nowhere did the raw earth appear." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0526.utt")
(utt.save (utt.synth (Utterance Text "The lush vegetation of that sheltered spot make a natural shield." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0527.utt")
(utt.save (utt.synth (Utterance Text "Men who endure it, call it living death." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0528.utt")
(utt.save (utt.synth (Utterance Text "As I say, he had tapped the message very rapidly." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0529.utt")
(utt.save (utt.synth (Utterance Text "Ask him, I laughed, then turned to Pasquini." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0530.utt")
(utt.save (utt.synth (Utterance Text "In what bucolic school of fence he had been taught was beyond imagining." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0531.utt")
(utt.save (utt.synth (Utterance Text "May drought destroy your crops." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0532.utt")
(utt.save (utt.synth (Utterance Text "Dunham, can your boy go along with Jesse." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0533.utt")
(utt.save (utt.synth (Utterance Text "But Johannes could, and did." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0534.utt")
(utt.save (utt.synth (Utterance Text "A new preacher and a new doctrine come to Jerusalem." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0535.utt")
(utt.save (utt.synth (Utterance Text "He would destroy all things that are fixed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0536.utt")
(utt.save (utt.synth (Utterance Text "He was an enthusiast and a desert dweller." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0537.utt")
(utt.save (utt.synth (Utterance Text "What Pascal glimpsed with the vision of a seer, I have lived." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0538.utt")
(utt.save (utt.synth (Utterance Text "I should like to engage just for one whole life in that." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0539.utt")
(utt.save (utt.synth (Utterance Text "Yea, so are all the lesser animals of today clean." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0540.utt")
(utt.save (utt.synth (Utterance Text "The Warden with a quart of champagne." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0541.utt")
(utt.save (utt.synth (Utterance Text "Without a doubt, some of them have dinner engagements." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0542.utt")
(utt.save (utt.synth (Utterance Text "I had been born with no organic, chemical predisposition toward alcohol." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0543.utt")
(utt.save (utt.synth (Utterance Text "He may anticipate the day of his death." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0544.utt")
(utt.save (utt.synth (Utterance Text "The Italian rancho was a bachelor establishment." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0545.utt")
(utt.save (utt.synth (Utterance Text "I lost my balance and pitched head foremost into the ooze." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0546.utt")
(utt.save (utt.synth (Utterance Text "Men like Joe Goose dated existence from drunk to drunk." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0547.utt")
(utt.save (utt.synth (Utterance Text "Also, churches and preachers I had never known." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0548.utt")
(utt.save (utt.synth (Utterance Text "Do you know that we weigh every pound of coal we burn." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0549.utt")
(utt.save (utt.synth (Utterance Text "This also became part of the daily schedule." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0550.utt")
(utt.save (utt.synth (Utterance Text "All an appearance can know is mirage." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0551.utt")
(utt.save (utt.synth (Utterance Text "Yet he dreams he is immortal, I argue feebly." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0552.utt")
(utt.save (utt.synth (Utterance Text "I am writing these lines in Honolulu, Hawaii." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0553.utt")
(utt.save (utt.synth (Utterance Text "Jack London, Waikiki Beach, Honolulu, Oahu." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0554.utt")
(utt.save (utt.synth (Utterance Text "Jerry was so secure in his nook that he did not roll away." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0555.utt")
(utt.save (utt.synth (Utterance Text "Why, he's bought forty pounds of goods from you already." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0556.utt")
(utt.save (utt.synth (Utterance Text "The last refugee had passed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0557.utt")
(utt.save (utt.synth (Utterance Text "And the foundation stone of service, in his case, was obedience." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0558.utt")
(utt.save (utt.synth (Utterance Text "Peace be unto you and grace before the Lord." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0559.utt")
(utt.save (utt.synth (Utterance Text "His mouth opened; words shaped vainly on his lips." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0560.utt")
(utt.save (utt.synth (Utterance Text "Bill lingered, contemplating his work with artistic appreciation." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0561.utt")
(utt.save (utt.synth (Utterance Text "What the flaming." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0562.utt")
(utt.save (utt.synth (Utterance Text "Mrs McFee's jaws brought together with a snap." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0563.utt")
(utt.save (utt.synth (Utterance Text "Then it is as I said, Womble announced with finality." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0564.utt")
(utt.save (utt.synth (Utterance Text "With them were Indians, also three other men." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0565.utt")
(utt.save (utt.synth (Utterance Text "Dennin's hands were released long enough for him to sign the document." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0566.utt")
(utt.save (utt.synth (Utterance Text "Now Irvine was a man of impulse, a poet." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0567.utt")
(utt.save (utt.synth (Utterance Text "He was just bursting with joy, joy over what." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0568.utt")
(utt.save (utt.synth (Utterance Text "At Lake Linderman I had one canoe, very good Peterborough canoe." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0569.utt")
(utt.save (utt.synth (Utterance Text "Behind him lay the thousand-years-long road across all Siberia and Russia." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0570.utt")
(utt.save (utt.synth (Utterance Text "He had forgotten to build a fire and thaw out." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0571.utt")
(utt.save (utt.synth (Utterance Text "I never saw anything like her in my life." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0572.utt")
(utt.save (utt.synth (Utterance Text "There was no law on the Yukon save what they made for themselves." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0573.utt")
(utt.save (utt.synth (Utterance Text "Good business man, Curly, O'Brien was saying." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0574.utt")
(utt.save (utt.synth (Utterance Text "There weren't any missions, and he was the man to know." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0575.utt")
(utt.save (utt.synth (Utterance Text "And the big Persian knew of his existence before he did of hers." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0576.utt")
(utt.save (utt.synth (Utterance Text "Once the jews harp began emitting its barbaric rhythms, Michael was helpless." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0577.utt")
(utt.save (utt.synth (Utterance Text "But we'll just postpone this." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0578.utt")
(utt.save (utt.synth (Utterance Text "There was the Emma Louisa." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0579.utt")
(utt.save (utt.synth (Utterance Text "This is my fifth voyage." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0580.utt")
(utt.save (utt.synth (Utterance Text "It was this proposition that started the big idea in Daughtry's mind." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0581.utt")
(utt.save (utt.synth (Utterance Text "Daughtry elaborated on the counting trick by bringing Cocky along." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0582.utt")
(utt.save (utt.synth (Utterance Text "Enjoy it he did, but principally for Steward's sake." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0583.utt")
(utt.save (utt.synth (Utterance Text "I have long noted your thirst unquenchable." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0584.utt")
(utt.save (utt.synth (Utterance Text "Wonder if he's a lion dog, Charles suggested." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0585.utt")
(utt.save (utt.synth (Utterance Text "We don't see ourselves as foolish." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0586.utt")
(utt.save (utt.synth (Utterance Text "He had comparatively no advantages at first." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0587.utt")
(utt.save (utt.synth (Utterance Text "He had proved it today, with his amateurish and sophomoric productions." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0588.utt")
(utt.save (utt.synth (Utterance Text "I was sick once -- typhoid." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0589.utt")
(utt.save (utt.synth (Utterance Text "In a way he is my protege." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0590.utt")
(utt.save (utt.synth (Utterance Text "We are both children together." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0591.utt")
(utt.save (utt.synth (Utterance Text "It's only his indigestion I find fault with." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0592.utt")
(utt.save (utt.synth (Utterance Text "She'd make a good wife for the cashier." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0593.utt")
(utt.save (utt.synth (Utterance Text "Gad, do I remember it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0001.utt")
(utt.save (utt.synth (Utterance Text "You got out by fighting, and I through a pretty girl." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0002.utt")
(utt.save (utt.synth (Utterance Text "I can see that knife now." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0003.utt")
(utt.save (utt.synth (Utterance Text "When I can't see beauty in woman I want to die." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0004.utt")
(utt.save (utt.synth (Utterance Text "His slim fingers closed like steel about Philip's." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0005.utt")
(utt.save (utt.synth (Utterance Text "He seized Gregson by the arm and led him to the door." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0006.utt")
(utt.save (utt.synth (Utterance Text "Hear the Indian dogs wailing down at Churchill." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0007.utt")
(utt.save (utt.synth (Utterance Text "Burke himself had criticized it because of the smile." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0008.utt")
(utt.save (utt.synth (Utterance Text "I'd say there was going to be a glorious scrap." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0009.utt")
(utt.save (utt.synth (Utterance Text "He turned the map to Gregson, pointing with his finger." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0010.utt")
(utt.save (utt.synth (Utterance Text "His eyes never took themselves for an instant from his companion's face." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0011.utt")
(utt.save (utt.synth (Utterance Text "Something that Whittemore had not yet said thrilled him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0012.utt")
(utt.save (utt.synth (Utterance Text "Lakes and rivers, hundreds of them, thousands of them." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0013.utt")
(utt.save (utt.synth (Utterance Text "Whitefish, Gregson, whitefish and trout." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0014.utt")
(utt.save (utt.synth (Utterance Text "They robbed me a few years later." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0015.utt")
(utt.save (utt.synth (Utterance Text "He chuckled as he pulled out his pipe and began filling it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0016.utt")
(utt.save (utt.synth (Utterance Text "Everything was working smoothly, better than I had expected." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0017.utt")
(utt.save (utt.synth (Utterance Text "I was completely lost in my work." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0018.utt")
(utt.save (utt.synth (Utterance Text "His slim hands gripped the edges of the table." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0019.utt")
(utt.save (utt.synth (Utterance Text "He made no reply as he waited for Whittemore to continue." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0020.utt")
(utt.save (utt.synth (Utterance Text "Philip dropped back into his chair." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0021.utt")
(utt.save (utt.synth (Utterance Text "If I was out of the game it would be easily made." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0022.utt")
(utt.save (utt.synth (Utterance Text "MacDougall, my engineer, believes it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0023.utt")
(utt.save (utt.synth (Utterance Text "It is growing, every day, every hour." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0024.utt")
(utt.save (utt.synth (Utterance Text "Now, you understand." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0025.utt")
(utt.save (utt.synth (Utterance Text "You have associated with some of these men." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0026.utt")
(utt.save (utt.synth (Utterance Text "And there's no chivalry, no quarter shown in this fight." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0027.utt")
(utt.save (utt.synth (Utterance Text "Lord Fitzhugh is the key to the whole situation." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0028.utt")
(utt.save (utt.synth (Utterance Text "All operations have been carried on from Montreal and Toronto." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0029.utt")
(utt.save (utt.synth (Utterance Text "Gregson held a lighted match until it burnt his fingertips." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0030.utt")
(utt.save (utt.synth (Utterance Text "Gregson had seated himself under the lamp and was sharpening a pencil." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0031.utt")
(utt.save (utt.synth (Utterance Text "He caught himself with a jerk." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0032.utt")
(utt.save (utt.synth (Utterance Text "How does your wager look now." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0033.utt")
(utt.save (utt.synth (Utterance Text "He confessed that the sketch had startled him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0034.utt")
(utt.save (utt.synth (Utterance Text "After all, the picture was only a resemblance." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0035.utt")
(utt.save (utt.synth (Utterance Text "He wondered, too, where Roscoe was." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0036.utt")
(utt.save (utt.synth (Utterance Text "Philip knew that she was not an Indian." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0037.utt")
(utt.save (utt.synth (Utterance Text "In her haste to get away she had forgotten these things." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0038.utt")
(utt.save (utt.synth (Utterance Text "Philip took a step toward Gregson, half determined to awaken him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0039.utt")
(utt.save (utt.synth (Utterance Text "The thought set his blood tingling." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0040.utt")
(utt.save (utt.synth (Utterance Text "But if Pierre did not return, until tomorrow." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0041.utt")
(utt.save (utt.synth (Utterance Text "Ten minutes had not elapsed since he had dropped the handkerchief." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0042.utt")
(utt.save (utt.synth (Utterance Text "It won't be for sale." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0043.utt")
(utt.save (utt.synth (Utterance Text "For a few moments he ate in silence." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0044.utt")
(utt.save (utt.synth (Utterance Text "Philip did not pursue the subject." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0045.utt")
(utt.save (utt.synth (Utterance Text "Philip produced a couple of cigars and took a chair opposite him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0046.utt")
(utt.save (utt.synth (Utterance Text "Suppose you saw me at work through the window." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0047.utt")
(utt.save (utt.synth (Utterance Text "He looked like one who had passed through an uncomfortable hour or two." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0048.utt")
(utt.save (utt.synth (Utterance Text "There was nothing more, except a large ink blot under the words." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0049.utt")
(utt.save (utt.synth (Utterance Text "All this day Gregson remained in the cabin." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0050.utt")
(utt.save (utt.synth (Utterance Text "The sixth day he spent in the cabin with Gregson." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0051.utt")
(utt.save (utt.synth (Utterance Text "The flush was gone from her face." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0052.utt")
(utt.save (utt.synth (Utterance Text "That is why I am, am rattled, he laughed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0053.utt")
(utt.save (utt.synth (Utterance Text "He understood the meaning of the look." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0054.utt")
(utt.save (utt.synth (Utterance Text "She was even more beautiful than when I saw her, before." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0055.utt")
(utt.save (utt.synth (Utterance Text "I'll give a thousand if you produce her, retorted Gregson." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0056.utt")
(utt.save (utt.synth (Utterance Text "They have won popular sentiment through the newspapers." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0057.utt")
(utt.save (utt.synth (Utterance Text "We must achieve our own salvation." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0058.utt")
(utt.save (utt.synth (Utterance Text "In moments of mental energy Philip was restless." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0059.utt")
(utt.save (utt.synth (Utterance Text "He would keep his faith with Gregson for the promised day or two." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0060.utt")
(utt.save (utt.synth (Utterance Text "Something about it seemed to fascinate him, to challenge his presence." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0061.utt")
(utt.save (utt.synth (Utterance Text "Now it was missing from the wall." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0062.utt")
(utt.save (utt.synth (Utterance Text "He boiled himself some coffee and sat down to wait." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0063.utt")
(utt.save (utt.synth (Utterance Text "I'm going down there with you, and I'm going to fight." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0064.utt")
(utt.save (utt.synth (Utterance Text "Now have you got anything to say against me, Mr Philip." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0065.utt")
(utt.save (utt.synth (Utterance Text "If I meet her again I shall apologize, said Eileen." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0066.utt")
(utt.save (utt.synth (Utterance Text "Below him the shadow was broken into a pool of rippling starlight." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0067.utt")
(utt.save (utt.synth (Utterance Text "Only the chance sound had led him to observe them." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0068.utt")
(utt.save (utt.synth (Utterance Text "Could the incident have anything to do with Jeanne and Pierre." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0069.utt")
(utt.save (utt.synth (Utterance Text "There was no chance to fire without hitting him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0070.utt")
(utt.save (utt.synth (Utterance Text "There was no answer from the other side." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0071.utt")
(utt.save (utt.synth (Utterance Text "Then he hastened on, as Pierre had guided him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0072.utt")
(utt.save (utt.synth (Utterance Text "With these arguments he convinced himself that he should go on alone." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0073.utt")
(utt.save (utt.synth (Utterance Text "Yet, behind them there was another and more powerful motive." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0074.utt")
(utt.save (utt.synth (Utterance Text "In that case he could not miss them, if he used caution." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0075.utt")
(utt.save (utt.synth (Utterance Text "Before Philip could recover himself Jeanne's startled guards were upon him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0076.utt")
(utt.save (utt.synth (Utterance Text "It is the nearest refuge." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0077.utt")
(utt.save (utt.synth (Utterance Text "There was pride and strength, the ring of triumph in his voice." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0078.utt")
(utt.save (utt.synth (Utterance Text "The truth of it set Jeanne quivering." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0079.utt")
(utt.save (utt.synth (Utterance Text "Tomorrow it will be strong enough for you to stand upon." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0080.utt")
(utt.save (utt.synth (Utterance Text "You were going to leave after you saw me on the rock." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0081.utt")
(utt.save (utt.synth (Utterance Text "He bit his tongue, and cursed himself at this fresh break." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0082.utt")
(utt.save (utt.synth (Utterance Text "In it there was something that was almost tragedy." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0083.utt")
(utt.save (utt.synth (Utterance Text "Your face is red with blood." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0084.utt")
(utt.save (utt.synth (Utterance Text "Her eyes smiled truth at him as he came up the bank." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0085.utt")
(utt.save (utt.synth (Utterance Text "He can care for himself." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0086.utt")
(utt.save (utt.synth (Utterance Text "They will search for us between their camp and Churchill." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0087.utt")
(utt.save (utt.synth (Utterance Text "Until I die, he exclaimed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0088.utt")
(utt.save (utt.synth (Utterance Text "Her beautiful hair was done up in shining coils." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0089.utt")
(utt.save (utt.synth (Utterance Text "The Churchill narrowed and its current became swifter as they progressed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0090.utt")
(utt.save (utt.synth (Utterance Text "For a full half minute Jeanne looked at him without speaking." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0091.utt")
(utt.save (utt.synth (Utterance Text "I want to die in it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0092.utt")
(utt.save (utt.synth (Utterance Text "Darkness hid him from Jeanne." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0093.utt")
(utt.save (utt.synth (Utterance Text "And yet if she came he had no words to say." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0094.utt")
(utt.save (utt.synth (Utterance Text "He heard a sound which brought him quickly into consciousness of day." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0095.utt")
(utt.save (utt.synth (Utterance Text "Within himself he called it no longer his own." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0096.utt")
(utt.save (utt.synth (Utterance Text "Besides, that noise makes me deaf." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0097.utt")
(utt.save (utt.synth (Utterance Text "Philip looked back from the crest and saw Jeanne leaning over the canoe." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0098.utt")
(utt.save (utt.synth (Utterance Text "Fifty yards ahead of her were the first of the rocks." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0099.utt")
(utt.save (utt.synth (Utterance Text "There was one chance, and only one, of saving Jeanne." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0100.utt")
(utt.save (utt.synth (Utterance Text "You're a devil for fighting, and will surely win." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0101.utt")
(utt.save (utt.synth (Utterance Text "I'll only be in the way." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0102.utt")
(utt.save (utt.synth (Utterance Text "He lifted his eyes, and a strange cry burst from his lips." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0103.utt")
(utt.save (utt.synth (Utterance Text "Shooting pains passed like flashes of electricity through his body." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0104.utt")
