(utt.save (utt.synth (Utterance Text "How old are you, mother." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0336.utt")
(utt.save (utt.synth (Utterance Text "Here he got a fresh thrill." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0337.utt")
(utt.save (utt.synth (Utterance Text "It was unobtrusive, yet it was there." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0338.utt")
(utt.save (utt.synth (Utterance Text "Well, I'll be plumb gosh darned." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0339.utt")
(utt.save (utt.synth (Utterance Text "These quick little joys of hers were sources of joy to him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0340.utt")
(utt.save (utt.synth (Utterance Text "I play that choice wide open to win." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0341.utt")
(utt.save (utt.synth (Utterance Text "Each improvement makes the value of everything else pump up." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0342.utt")
(utt.save (utt.synth (Utterance Text "But how are you going to do it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0343.utt")
(utt.save (utt.synth (Utterance Text "Lots of men take women buggy riding." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0344.utt")
(utt.save (utt.synth (Utterance Text "Daylight made no answer, and the door closed behind him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0345.utt")
(utt.save (utt.synth (Utterance Text "There's not an iota of truth in it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0346.utt")
(utt.save (utt.synth (Utterance Text "But ever his gaze returned to that Crouched Venus on the piano." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0347.utt")
(utt.save (utt.synth (Utterance Text "Would you be satisfied with that one hundredth part of me." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0348.utt")
(utt.save (utt.synth (Utterance Text "In such a tumbling of values was no time to sell." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0349.utt")
(utt.save (utt.synth (Utterance Text "Stand off butcher and baker and all the rest." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0350.utt")
(utt.save (utt.synth (Utterance Text "Matthewson, who's this bookkeeper, Rogers." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0351.utt")
(utt.save (utt.synth (Utterance Text "Now just what do you want to know." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0352.utt")
(utt.save (utt.synth (Utterance Text "I want to know how all this is possible." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0353.utt")
(utt.save (utt.synth (Utterance Text "It's that much junk." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0354.utt")
(utt.save (utt.synth (Utterance Text "There was proper division of labor in the work they individually performed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0355.utt")
(utt.save (utt.synth (Utterance Text "He loved to play Chinese lottery." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0356.utt")
(utt.save (utt.synth (Utterance Text "The Law of Club and Fang" )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0357.utt")
(utt.save (utt.synth (Utterance Text "The temperature dropped to fifty below zero and remained there the whole trip." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0358.utt")
(utt.save (utt.synth (Utterance Text "And as never before, he was ready to obey." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0359.utt")
(utt.save (utt.synth (Utterance Text "This state of mind comes of an undue prominence of the ego." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0360.utt")
(utt.save (utt.synth (Utterance Text "There are the canals of China, and the Yang-tse River." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0361.utt")
(utt.save (utt.synth (Utterance Text "We threaten to be of the one mind before the voyage is completed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0362.utt")
(utt.save (utt.synth (Utterance Text "She was built primarily to sail." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0363.utt")
(utt.save (utt.synth (Utterance Text "In the Bohemian Club of San Francisco there are some crack sailors." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0364.utt")
(utt.save (utt.synth (Utterance Text "My age, in years, is twenty two." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0365.utt")
(utt.save (utt.synth (Utterance Text "I forgot how easily I had taught myself from the printed page." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0366.utt")
(utt.save (utt.synth (Utterance Text "Any average young fellow can teach himself in a week." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0367.utt")
(utt.save (utt.synth (Utterance Text "Please do not think that I already know it all." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0368.utt")
(utt.save (utt.synth (Utterance Text "You see, we were teaching ourselves." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0369.utt")
(utt.save (utt.synth (Utterance Text "And now behold the perversity of things." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0370.utt")
(utt.save (utt.synth (Utterance Text "Yield yourself to the waters that are ripping and tearing at you." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0371.utt")
(utt.save (utt.synth (Utterance Text "Mr McVeigh told me about him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0372.utt")
(utt.save (utt.synth (Utterance Text "Unlike Joshua, he stood in no need of divine assistance." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0373.utt")
(utt.save (utt.synth (Utterance Text "To say the least, Captain Cook was a rather thorough going empiricist." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0374.utt")
(utt.save (utt.synth (Utterance Text "Man could not conquer them." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0375.utt")
(utt.save (utt.synth (Utterance Text "Thought I, and a worthy fool he proved." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0376.utt")
(utt.save (utt.synth (Utterance Text "A scarlet loincloth completed his costume." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0377.utt")
(utt.save (utt.synth (Utterance Text "I like to speculate upon the glorious future of man." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0378.utt")
(utt.save (utt.synth (Utterance Text "Christmas is an easy problem compared with a Polynesian giving-feast." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0379.utt")
(utt.save (utt.synth (Utterance Text "He had peeled off his shirt and was wildly waving it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0380.utt")
(utt.save (utt.synth (Utterance Text "And how would we ever find ourselves." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0381.utt")
(utt.save (utt.synth (Utterance Text "I defy any man to get a Solomon Island sore in California." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0382.utt")
(utt.save (utt.synth (Utterance Text "A bush chief had died a natural death." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0383.utt")
(utt.save (utt.synth (Utterance Text "The skipper's and Nakata's gymnastics served as a translation without words." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0384.utt")
(utt.save (utt.synth (Utterance Text "Last night he showed all the symptoms of coming down with pneumonia." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0385.utt")
(utt.save (utt.synth (Utterance Text "My idea was that he would have more influence over the natives." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0386.utt")
(utt.save (utt.synth (Utterance Text "It is merely the simple superlative." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0387.utt")
(utt.save (utt.synth (Utterance Text "I made no more overtures." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0388.utt")
(utt.save (utt.synth (Utterance Text "Among my minor afflictions, I may mention a new and mysterious one." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0389.utt")
(utt.save (utt.synth (Utterance Text "The voyage was our idea of a good time." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0390.utt")
(utt.save (utt.synth (Utterance Text "At sea, Tuesday, March 17, 1908." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0391.utt")
(utt.save (utt.synth (Utterance Text "Yes, sir, he answered, with cheerful alacrity." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0392.utt")
(utt.save (utt.synth (Utterance Text "I was still weak from my prolonged immersion." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0393.utt")
(utt.save (utt.synth (Utterance Text "The boy hesitated, then mastered his temper." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0394.utt")
(utt.save (utt.synth (Utterance Text "I was beneath the water, suffocating and drowning." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0395.utt")
(utt.save (utt.synth (Utterance Text "The pain from my hurt knee was agonizing." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0396.utt")
(utt.save (utt.synth (Utterance Text "The hunters were still arguing and roaring like some semi-human amphibious breed." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0397.utt")
(utt.save (utt.synth (Utterance Text "I have been robbed, sir, I amended." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0398.utt")
(utt.save (utt.synth (Utterance Text "You were looking squeamish this afternoon, he began." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0399.utt")
(utt.save (utt.synth (Utterance Text "How could I answer the question on the spur of the moment." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0400.utt")
(utt.save (utt.synth (Utterance Text "I learned it myself in English ships." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0401.utt")
(utt.save (utt.synth (Utterance Text "An altruistic act is an act performed for the welfare of others." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0402.utt")
(utt.save (utt.synth (Utterance Text "Knowing him, I review the old Scandinavian myths with clearer understanding." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0403.utt")
(utt.save (utt.synth (Utterance Text "Yes, and no, sir, was the slow reply." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0404.utt")
(utt.save (utt.synth (Utterance Text "And each year something happened, and I did not go." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0405.utt")
(utt.save (utt.synth (Utterance Text "How in hell did he know it was you in the dark." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0406.utt")
(utt.save (utt.synth (Utterance Text "Of course much grumbling went on, and little outbursts were continually occurring." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0407.utt")
(utt.save (utt.synth (Utterance Text "You have all the advantage." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0408.utt")
(utt.save (utt.synth (Utterance Text "The dirk mentioned by Wolf Larsen rested in its sheath on my hip." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0409.utt")
(utt.save (utt.synth (Utterance Text "Have you ever earned a dollar by your own labour." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0410.utt")
(utt.save (utt.synth (Utterance Text "He gave no reason, but his motive was obvious enough." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0411.utt")
(utt.save (utt.synth (Utterance Text "Ah, it was sweet in my ears." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0412.utt")
(utt.save (utt.synth (Utterance Text "He may desire to escape pain, or to enjoy pleasure." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0413.utt")
(utt.save (utt.synth (Utterance Text "It was impossible to hoist sail and claw off that shore." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0414.utt")
(utt.save (utt.synth (Utterance Text "There was nothing for us but the wide raw ocean." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0415.utt")
(utt.save (utt.synth (Utterance Text "I arose obediently and went down the beach." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0416.utt")
(utt.save (utt.synth (Utterance Text "The next thing to watch out for is bed sores." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0417.utt")
(utt.save (utt.synth (Utterance Text "At that moment I got the impression that she was willowy." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0418.utt")
(utt.save (utt.synth (Utterance Text "Your father's fifth command, he nodded." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0419.utt")
(utt.save (utt.synth (Utterance Text "On occasion, on this traverse, the Cape Verde Islands are raised." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0420.utt")
(utt.save (utt.synth (Utterance Text "She is essentially the life-giving, life-conserving female of the species." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0421.utt")
(utt.save (utt.synth (Utterance Text "This was when the explosion occurred." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0422.utt")
(utt.save (utt.synth (Utterance Text "Also, at regular intervals, he would mutter." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0423.utt")
(utt.save (utt.synth (Utterance Text "It is a very tenable hypothesis, and will bear looking into." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0424.utt")
(utt.save (utt.synth (Utterance Text "There were orange-green, gold-green, and a copper-green." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0425.utt")
(utt.save (utt.synth (Utterance Text "The Gabriel voice of the Samurai rang out." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0426.utt")
(utt.save (utt.synth (Utterance Text "The sunsets grow more bizarre and spectacular off this coast of the Argentine." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0427.utt")
(utt.save (utt.synth (Utterance Text "The history of our westward-faring race is written in it." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0428.utt")
(utt.save (utt.synth (Utterance Text "And the Eurasian Chinese-Englishman bowed himself away." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0429.utt")
(utt.save (utt.synth (Utterance Text "They were babbling and chattering all together." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0430.utt")
(utt.save (utt.synth (Utterance Text "Too much, he told me, with ominous rolling head." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0431.utt")
(utt.save (utt.synth (Utterance Text "He is a candidate, rising from the serf class to our class." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0432.utt")
(utt.save (utt.synth (Utterance Text "We are cooking on the coal stove and on the oil burners." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0433.utt")
(utt.save (utt.synth (Utterance Text "The steward has just tendered me a respectful bit of advice." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0434.utt")
(utt.save (utt.synth (Utterance Text "Well, did they eat." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0435.utt")
(utt.save (utt.synth (Utterance Text "Famine had been my great ally." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0436.utt")
(utt.save (utt.synth (Utterance Text "Nowhere in the North is the soil so prolific." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0437.utt")
(utt.save (utt.synth (Utterance Text "They ran the canoe in and climbed the high earth bank." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0438.utt")
(utt.save (utt.synth (Utterance Text "The land exchanged its austere robes for the garb of a smiling wanton." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0439.utt")
(utt.save (utt.synth (Utterance Text "There were stir and bustle, new faces, and fresh facts." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0440.utt")
(utt.save (utt.synth (Utterance Text "And there was Ethel Baird, whom also you must remember." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0441.utt")
(utt.save (utt.synth (Utterance Text "He had become a man very early in life." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0442.utt")
(utt.save (utt.synth (Utterance Text "I did not think you would be so early." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0443.utt")
(utt.save (utt.synth (Utterance Text "He did not know what went on in the minds of his superiors." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0444.utt")
(utt.save (utt.synth (Utterance Text "Mops, sir, eagerly answered the sailor at the wheel." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0445.utt")
(utt.save (utt.synth (Utterance Text "Some one had thrust a large sheath-knife into his hand." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0446.utt")
(utt.save (utt.synth (Utterance Text "O'Brien emitted a shriek that sank swiftly to a gurgling sob." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0447.utt")
(utt.save (utt.synth (Utterance Text "Sandel would never become a world champion." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0448.utt")
(utt.save (utt.synth (Utterance Text "Also, she wouldn't walk." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0449.utt")
(utt.save (utt.synth (Utterance Text "To my dearest and always appreciated friend, I submit myself." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0450.utt")
(utt.save (utt.synth (Utterance Text "You used to joyride like the very devil." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0451.utt")
(utt.save (utt.synth (Utterance Text "They saw each other for the first time in Boston." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_b0452.utt")
