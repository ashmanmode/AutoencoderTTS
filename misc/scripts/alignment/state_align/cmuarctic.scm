(utt.save (utt.synth (Utterance Text "I had forgotten their existence." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0460.utt")
(utt.save (utt.synth (Utterance Text "Ah, we were very close together in that moment." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0461.utt")
(utt.save (utt.synth (Utterance Text "But she swung obediently on her heel into the wind." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0462.utt")
