(utt.save (utt.synth (Utterance Text "He spat it out like so much venom." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0470.utt")
(utt.save (utt.synth (Utterance Text "I saw Mr Pike nod his head grimly and sarcastically." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0471.utt")
(utt.save (utt.synth (Utterance Text "He is too keenly intelligent, too sharply sensitive, successfully to endure." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0472.utt")
