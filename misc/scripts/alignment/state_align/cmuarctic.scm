(utt.save (utt.synth (Utterance Text "Your price, my son, is just about thirty per week." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0376.utt")
(utt.save (utt.synth (Utterance Text "This sound did not disturb the hush and awe of the place." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0377.utt")
