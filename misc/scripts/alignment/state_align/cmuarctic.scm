(utt.save (utt.synth (Utterance Text "I obeyed, and a minute or two later they stood before him." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0455.utt")
(utt.save (utt.synth (Utterance Text "But it won't continue, she said with easy confidence." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0456.utt")
(utt.save (utt.synth (Utterance Text "What I saw I could not at first believe." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0457.utt")
(utt.save (utt.synth (Utterance Text "The stout wood was crushed like an eggshell." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0458.utt")
