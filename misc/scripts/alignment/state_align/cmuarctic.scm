(utt.save (utt.synth (Utterance Text "And so early in the voyage, too." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0467.utt")
(utt.save (utt.synth (Utterance Text "In the matter of curry she is a sheer genius." )) "/home/ashish/Text2Speech/TTSDNNRepo/misc/scripts/alignment/state_align/prompt-utt/arctic_a0468.utt")
